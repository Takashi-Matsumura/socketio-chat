generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SurveyResult {
  id        Int    @id @default(autoincrement())
  surveyId  String
  ans1      Int
  ans2      Int
  ans3      Int
  ans4      Int
  ans5      Int
  createdAt DateTime @default(now())
}